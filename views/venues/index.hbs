<h1>List Venues</h1>

<div>
  {{#each venues}}
  <div>Name: {{this.name}}</div>
  <div>Adress: {{this.address}}</div>
  <div>Phone: {{this.phone}}</div>
  <div>dtsa: {{this.dtsa}}</div>
  <div><a href='/users/{{../user._id}}/venues/{{this._id}}/games'>( games )</a></div>
  <div><a href='/users/'>( back to users )</a></div>

  {{/each}}
</div>